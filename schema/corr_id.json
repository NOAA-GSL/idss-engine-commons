{
    "CorrId": {
        "description": "Correlation identification used to track flow through the microservices",
        "type": "object",
        "properties": {
            "originator": {
                "type": "string",
                "enum": ["IDSSe", "IMS"]
            },
            "uuid": {
                "type": "string",
                "format": "uuid"
            },
            "issueDt": {"$ref": "timing.json#/TimeString"}
        },
        "required": [
            "originator",
            "uuid",
            "issueDt"
        ]
    }
}